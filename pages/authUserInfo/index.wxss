.loginAuthorization {
  padding: 200rpx 32rpx;
}
.newloginBox {
  width: 100%;
}
.loginInfoBox {
  width: 100%;
  height: 210rpx;
  float: left;
  margin-top: 86rpx;
}
.loginNameBox,
.loginIconBox {
  width: 100%;
  float: left;
  text-align: center;
}
.loginIconBox {
  height: 146rpx;
}
.loginIconBox img {
  width: 146rpx;
  height: 146rpx;
}
.loginNameBox {
  height: 38rpx;
  margin-top: 26rpx;
  font-size: 40rpx;
  line-height: 40rpx;
  font-weight: 600;
  color: #2c2c2c;
}
.dividingLine {
  width: 100%;
  height: 1rpx;
  background: url(https://static.techmini.cn/I2awPX0805170059.png) repeat-x
    center;
  background-size: 100% 1rpx;
  float: left;
  margin-top: 70rpx;
  margin-bottom: 62rpx;
}
.loginTextBox {
  float: left;
  width: 100%;
  margin-bottom: 69rpx;
}
.loginTextBox p {
  float: left;
  font-size: 32rpx;
  font-weight: 600;
  color: #2c2c2c;
  margin-bottom: 46rpx;
  line-height: 40rpx;
  margin-left: 41rpx;
  margin-right: 33rpx;
}
.loginTextBox span {
  float: left;
  font-size: 30rpx;
  color: #666666;
  line-height: 30rpx;
  margin-left: 41rpx;
}
.loginBtn {
  width: 100%;
  height: 100rpx;
  background: #06c261;
  border-radius: 10rpx;
  font-size: 36rpx;
  line-height: 100rpx;
  color: #ffffff;
}
